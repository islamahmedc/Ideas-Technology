/********************************************************************************
***** Includes                  										    *****
********************************************************************************/
#include "avr/delay.h"
#include "DIO_Configurations.h"
#include "DIO_Interfacing.h"

/********************************************************************************
***** Main Function               										    *****
********************************************************************************/
void main (void){
	//Set direction of PIN A1 to be output
	DIO_voidSetDirection(DIO_u8PinA1,DIO_u8PinOp);

	while(1){
		DIO_voidWritePIN(DIO_u8PinA1,DIO_u8HIGH);
		_
	}
}
